{
  "name": "decompile-wx",
  "version": "1.0.0",
  "main": "dist/main.bundle.js",
  "repository": "https://github.com/shunyue1320/decompile-wx.git",
  "author": "shunyue1320",
  "license": "MIT",
  "scripts": {
    "start:view": "cross-env NODE_ENV=development webpack serve --config webpack.view.config.js --watch",
    "start:electron": "cross-env NODE_ENV=development webpack --config webpack.electron.config.js && electron .",
    "bulid:view": "cross-env NODE_ENV=production webpack --config webpack.view.config.js",
    "bulid:electron": "cross-env NODE_ENV=production webpack --config webpack.electron.config.js",
    "build": "yarn bulid:view && yarn bulid:electron && cross-env NODE_ENV=production electron-builder --win --x64"
  },
  "build": {
    "appId": "com.guojianbo.top",
    "productName": "小程序反编译",
    "copyright": "Copyright © 2021 ${author}",
    "directories": {
      "output": "build"
    },
    "files": [
      "./package.json",
      "./dist/**"
    ],
    "win": {
      "icon": "./src/views/assets/images/logo.ico",
      "artifactName": "${productName}_${version}.${ext}"
    },
    "mac": {
      "artifactName": "${productName}_${version}.${ext}"
    }
  },
  "keywords": [
    "反编译",
    "wxapkg",
    "微信小程序",
    "miniprogram"
  ],
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "ant-design-vue": "^2.0.0",
    "bytenode": "^1.1.7",
    "cheerio": "^1.0.0-rc.5",
    "child_process": "^1.0.2",
    "css-tree": "^1.0.0-alpha.28",
    "cssbeautify": "^0.3.1",
    "escodegen": "^2.0.0",
    "esprima": "^4.0.0",
    "js-beautify": "^1.7.5",
    "uglify-es": "^3.3.9",
    "vm2": "^3.6.0",
    "vue": "^3.0.5",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.10",
    "@vue/compiler-sfc": "^3.0.5",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "dotenv-expand": "^5.1.0",
    "dotenv-flow": "^3.2.0",
    "electron": "9.4.0",
    "html-webpack-plugin": "5.0.0-beta.6",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "vue-loader": "^16.1.2",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-node-externals": "^2.5.2"
  }
}
